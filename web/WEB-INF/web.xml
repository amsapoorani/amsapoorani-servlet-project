    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStudetail</servlet-name>
        <url-pattern>/UpdateStudetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Loginverification</servlet-name>
        <url-pattern>/Loginverification</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginservlet</servlet-name>
        <url-pattern>/loginservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
