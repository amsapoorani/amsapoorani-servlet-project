        <servlet-class>studentregistrationdetail.Viewspecificrecord</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteSpecificServlet</servlet-name>
        <servlet-class>studentregistrationdetail.DeleteSpecificServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStudetail</servlet-name>
        <servlet-class>studentregistrationdetail.UpdateStudetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Loginverification</servlet-name>
        <servlet-class>studentregistrationdetail.Loginverification</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginservlet</servlet-name>
        <servlet-class>studentregistrationdetail.loginservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Studentsreg</servlet-name>
        <url-pattern>/Studentsreg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewStudentreg</servlet-name>
        <url-pattern>/ViewStudentreg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Viewspecificrecord</servlet-name>
        <url-pattern>/Viewspecificrecord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteSpecificServlet</servlet-name>
